clean_win__32_bit__release: 
	cmd /c del /f $(OBJ_WIN__32_BIT__RELEASE) $(OUT_WIN__32_BIT__RELEASE)
	cmd /c rd bin\\Release
	cmd /c rd $(OBJDIR_WIN__32_BIT__RELEASE)\\__\\src

.PHONY: before_win_32_bit__debug after_win_32_bit__debug clean_win_32_bit__debug before_win__32_bit__release after_win__32_bit__release clean_win__32_bit__release

